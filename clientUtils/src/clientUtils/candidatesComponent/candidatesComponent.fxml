<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="rootVBox" alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clientUtils.candidatesComponent.CandidatesComponentController">
   <children>
      <Label text="Candidates">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TableView fx:id="candidatesTableView" maxHeight="-Infinity" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="500.0">
        <columns>
          <TableColumn fx:id="allyColumn" editable="false" prefWidth="75.0" sortable="false" text="ally" />
          <TableColumn fx:id="candidateColumn" editable="false" prefWidth="75.0" sortable="false" text="candidate" />
            <TableColumn fx:id="codeColumn" editable="false" prefWidth="150.0" sortable="false" text="code" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <padding>
      <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
   </padding>
</VBox>
