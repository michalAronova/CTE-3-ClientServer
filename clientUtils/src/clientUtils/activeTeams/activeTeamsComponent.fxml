<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<ScrollPane fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clientUtils.activeTeams.ActiveTeamsComponentController">
   <content>
      <VBox fx:id="rootVBox" alignment="CENTER">
         <children>
            <Label text="Active Teams">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <TableView fx:id="teamsTableView" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="200.0" minWidth="300.0" prefHeight="200.0" prefWidth="300.0">
              <columns>
                <TableColumn fx:id="nameColumn" editable="false" minWidth="30.0" prefWidth="-1.0" sortable="false" text="name" />
                <TableColumn fx:id="agentAmountColumn" editable="false" minWidth="5.0" prefWidth="-1.0" sortable="false" text="Agent Amount" />
                  <TableColumn fx:id="missionSizeColumn" editable="false" minWidth="5.0" prefWidth="-1.0" sortable="false" text="Mission Size" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
         <padding>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </padding>
      </VBox>
   </content>
</ScrollPane>
