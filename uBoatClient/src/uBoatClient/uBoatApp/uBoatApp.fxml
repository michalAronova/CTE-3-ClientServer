  <content>
      <VBox fx:id="vBox" alignment="CENTER">
         <children>
            <Label text="Welcome, UBOAT!">
               <font>
                  <Font name="Bookman Old Style" size="20.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Label>
            <fx:include fx:id="chooseNameComponent" source="../../../../clientUtils/src/clientUtils/chooseNameComponent/chooseNameComponent.fxml" />
         </children>
      </VBox>
  </content>
</ScrollPane>
