<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>


<VBox fx:id="rootVBox" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="alliesClient.components.agentDisplay.AgentDisplayController">
   <children>
      <Label text="My Agents">
         <VBox.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </VBox.margin>
      </Label>
      <TableView fx:id="agentsTableView" prefHeight="170.0" prefWidth="270.0">
        <columns>
          <TableColumn fx:id="nameColumn" minWidth="50.0" text="name" />
          <TableColumn fx:id="threadAmountColumn" prefWidth="50.0" text="Threads" />
            <TableColumn fx:id="missionPullColumn" prefWidth="75.0" text="Mission Pull" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</VBox>
